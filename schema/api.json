      "type": [
        "object"
      ],
      "definitions": {
        "identifier": {
          "description": "shopify URL handler",
          "readOnly": true,
          "type": [
            "string"
          ]
        },
        "id": {
          "description": "the product id",
          "readOnly": true,
          "type": [
            "integer"
          ]
        },
        "title": {
          "description": "the product title",
          "readOnly": true,
          "type": [
            "string"
          ]
        },
        "image": {
          "description": "the product image",
          "readOnly": true,
          "type": [
            "string"
          ]
        },
        "price": {
          "description": "the product price",
          "readOnly": true,
          "type": [
            "string"
          ]
        },
        "identity": {
          "anyOf": [
            {
              "$ref": "#/definitions/product/definitions/id"
            }
